[

  {
    "description": "会返回cookies信息的get请求",
    "request": {
      "uri": "/getcookies",
      "method": "get"
    },
    "response": {
      "text": "恭喜获得cookies信息成功",
      "cookies": {
        "login": "true",
        "status": "10000"
      }
    }
}
  ,
  {
    "description": "带cookies信息的get请求",
    "request": {
      "uri": "/get/with/cookies",
      "method": "get",
      "cookies": {
        "login": "true"
      }
    },
    "response": {
      "text": "这是一个需要携带cookies信息才能访问的get请求"
    }

  },
  {
    "description": "带cookies信息的post请求",
    "request": {
      "uri": "/post/with/cookies",
      "method": "post",
      "cookies": {
        "login": "true"
      },
      "json": {
        "name": "huhansan",
        "age": "18"
      }
    },
    "response": {
      "status": 200,
      "json": {
        "huhansan": "success",
        "haha": "heiheihei"
      }
    }

  }


]